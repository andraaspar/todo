{
	"name": "todo",
	"version": "0.1.0",
	"homepage": "https://andraaspar.github.io/todo",
	"private": true,
	"dependencies": {
		"@parcel/service-worker": "2.5.0",
		"@parcel/validator-typescript": "2.5.0",
		"gh-pages": "^4.0.0",
		"matul": "^0.0.8",
		"parcel": "2.5.0",
		"rimraf": "^3.0.2",
		"typescript": "^4.7.3"
	},
	"scripts": {
		"start": "rimraf public && parcel -p 8080 --dist-dir public src/index.html",
		"build": "rimraf public && parcel build --public-url \".\" --dist-dir public --no-source-maps src/index.html",
		"deploy": "gh-pages -d public"
	},
	"devDependencies": {
		"@parcel/packager-raw-url": "2.5.0",
		"@parcel/transformer-inline-string": "2.5.0",
		"@parcel/transformer-sass": "2.5.0",
		"@parcel/transformer-typescript-tsc": "2.5.0",
		"@parcel/transformer-webmanifest": "2.5.0"
	}
}
